{
  "name": "metamodels/notelist",
  "description": "Notelist management for MetaModels",
  "license": "LGPL-3.0-or-later",
  "keywords":["contao", "metamodels", "notelist"],
  "type":"contao-module",
  "homepage":"http://now.metamodel.me/",
  "authors":[
    {
      "name":"Christian Schiffler",
      "email":"c.schiffler@cyberspectrum.de",
      "homepage":"http://www.cyberspectrum.de",
      "role":"Developer"
    },
    {
      "name":"Ingolf Steinhardt",
      "email":"info@e-spin.de",
      "homepage":"http://www.e-spin.de",
      "role":"Developer"
    }
  ],
  "support":{
    "email":"mail@metamodel.me",
    "issues":"https://github.com/MetaModels/metamodels_notelist",
    "wiki":"http://de.contaowiki.org/MetaModels",
    "irc":"irc://irc.freenode.org/contao.mm",
    "source":"https://github.com/MetaModels/metamodels_notelist"
  },
  "require":{
    "php": "^7.0",
    "contao-community-alliance/dc-general": "^2.1",
    "contao-community-alliance/url-builder": "^1.3",
    "contao/core-bundle": "^4.4",
    "menatwork/contao-multicolumnwizard-bundle": "^3.4",
    "metamodels/core":"^2.1",
    "symfony/event-dispatcher": "^2.8 | ^3.0 | ^4.0"
  },
  "require-dev": {
    "cyberspectrum/contao-toolbox": "^0.7",
    "phpcq/all-tasks": "^1.2",
    "contao/manager-plugin": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "MetaModels\\NoteListBundle\\": "src"
    },
    "files": [
      "src/deprecated-autoload.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "MetaModels\\NoteListBundle\\Test\\": "tests"
    }
  },
  "extra":{
    "contao-manager-plugin": "MetaModels\\NoteListBundle\\ContaoManager\\Plugin",
    "contao": {
      "transifex": {
        "project": "metamodels",
        "prefix": "notelist-",
        "languages_cto": "contao/languages",
        "languages_tx": ".tx"
      }
    },
    "branch-alias": {
      "dev-hotfix/2.1.2": "2.1.2-dev"
    }
  }
}
